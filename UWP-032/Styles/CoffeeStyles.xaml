<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style 
        TargetType="Image"
        x:Key="WhiteLogo">
        <Setter Property="Source" Value="../Assets/CurrentAssets/white-logo.png"/>
    </Style>
    <Style 
        TargetType="Grid"
        x:Key="Grid">
        <Setter Property="Background" Value="#3C1F19"/>
    </Style>
    <Style 
        TargetType="Button"
        x:Key="CoffeeFlyoutButton">
        <Setter Property="Background" Value="#301914"/>
        <Setter Property="Foreground" Value="White"/>
    </Style>
    <Style 
        TargetType="TextBlock" 
        x:Key="ResultRowTextBlocks">
        <Setter Property="Foreground" Value="White"/>
        <Setter Property="Grid.Row" Value="2"/>
    </Style>
</ResourceDictionary>
